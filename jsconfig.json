{
	"compilerOptions": {
	  "target": "ESNext",
	  "useDefineForClassFields": true,
	  "module": "ESNext",
	  "lib": ["ESNext", "DOM"],
	  "moduleResolution": "bundler",
	  "strict": true,
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "esModuleInterop": true,
	  "noEmit": true,
	  "skipLibCheck": true
	},
	"include": ["src", "debug"]
}